algoritmo "1)a"
  var n1,n2 : inteiro
 inicio
  escreval("Digite um numero")
    leia(n1)
   escreval("Digite outro numero diferente do primeiro")
      leia(n2)
   se n1>n2 entao
         escreval("Esse e o maior:  ",n1)
     senao
          escreval("Esse e o maior:  ",n2)
  se n1=n2 entao
      escreval("Erro!!! pois os numeros sao iguais")
     fimse
  fimse
  fimalgoritmo
